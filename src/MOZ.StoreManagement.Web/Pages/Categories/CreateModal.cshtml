@page
@using MOZ.StoreManagement.Localization
@using MOZ.StoreManagement.Web.Pages.Categories
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModal
@inject IStringLocalizer<StoreManagementResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Book1" asp-page="/Categories/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewCategory"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
